            "settings": {
                "foreground": "#227C9D"
            }
        },
        {
            "name": "Inserted",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#17C3B2"
            }
        },
        {
            "name": "Deleted",
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#FE6D73"
            }
        },
        {
            "name": "Changed",
